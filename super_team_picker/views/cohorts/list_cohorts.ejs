<%- include("../partials/header") %>
<% for(let cohort of cohorts){ %>
<form action="/cohorts" method="POST">
  <div>
    <nav class="nav_cohort">
        <a class="navbar-brand" href="#">
        <% if(cohort.logo_url){ %>
        <img src="<%= cohort.logo_url %>" height="50" width="50">
         <% } %>    
        </a>
        <a class="cohort-name-form" href="/home">
        <h2><%= cohort.name %></h2>
        </a>
    </nav>
  </div>
  <p><%= cohort.members%></p>
  <input type="submit" value="Submit!">
</form>
<% } %>

<%- include("../partials/footer") %>
